<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/toolbar" />

    <TextView
        android:id="@+id/remark_buttom"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:background="#FFF8D6"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:text=""
        android:gravity="center"/>

    <com.rubu.view.PullDownScrollView
        android:id="@+id/mPullDownScrllView"
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_weight="1"
        android:background="@color/white"
        android:orientation="vertical">

        <ScrollView
            android:id="@+id/scrollView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bg_color"
            android:scrollbars="none">

            <LinearLayout
                android:id="@+id/view_root"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp"
                android:background="@android:color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/act_pick_item_margin"
                    android:paddingRight="@dimen/act_pick_item_margin">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/item_margin_top">

                        <TextView
                            android:id="@+id/tv_parent_wk_order_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="安装单"
                            android:textColor="@color/text_blue"
                            android:textSize="18sp" />

                        <TextView
                            android:id="@+id/tv_yes_no_quickness"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="4dp"
                            android:text="极速安装"
                            android:textColor="@color/ucrop_color_widget_active"
                            android:textSize="@dimen/title_text_size"
                            android:visibility="gone" />

                    </LinearLayout>

                    <TextView
                    android:id="@+id/tv_parent_wk_order_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="TM121491I93I192I3"
                    android:textColor="@color/text_blue"
                    android:textSize="@dimen/text_medium" />

                    <TextView
                        android:id="@+id/tv_order_arrival_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="@color/text_blue"
                        android:textSize="@dimen/text_medium"
                        android:visibility="gone"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/item_margin_top"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_buyer_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="王先森"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_large" />

                        <TextView
                            android:id="@+id/tv_buyer_mobile"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="18790851899"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_medium" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_subscribe_hint"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/item_margin_top"
                        android:background="@drawable/bg_pick_finsih"
                        android:gravity="left"
                        android:padding="10dp"
                        android:text="@string/subscribe_hint"
                        android:textColor="@color/red_dark"
                        android:visibility="gone" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/div_height"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="8dp"
                        android:background="@color/gray_light" />


                    <LinearLayout
                        android:id="@+id/ll_adress"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_service_address_title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:layout_weight="1"
                                android:text="服务地址:"
                                android:textColor="@color/black"
                                android:textSize="@dimen/text_large" />

                            <TextView
                                android:id="@+id/tv_guide"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="4dp"
                                android:layout_weight="1"
                                android:text="点击地址导航"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="@dimen/text_large" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/tv_buyer_address_tag"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/item_margin_top"
                            android:layout_weight="1"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_medium" />

                        <TextView
                            android:id="@+id/tv_buyer_address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/item_margin_top"
                            android:layout_weight="1"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_medium" />
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/ll_real_adress"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_real_service_address_title"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="4dp"
                                android:layout_weight="1"
                                android:text="修正地址:"
                                android:textColor="@color/black"
                                android:textSize="@dimen/text_large" />

                            <TextView
                                android:id="@+id/tv_real_guide"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20dp"
                                android:layout_marginTop="4dp"
                                android:layout_weight="1"
                                android:text="点击地址导航"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="@dimen/text_large" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/tv_real_buyer_address_tag"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/item_margin_top"
                            android:layout_weight="1"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_medium" />

                        <TextView
                            android:id="@+id/tv_real_buyer_address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/item_margin_top"
                            android:layout_weight="1"
                            android:textColor="@color/gray_dark"
                            android:textSize="@dimen/text_medium" />
                    </LinearLayout>


                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/div_height"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="8dp"
                        android:background="@color/gray_light" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="6dp"
                            android:layout_marginTop="6dp"
                            android:layout_weight="1"
                            android:text="@string/service_goods"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_large" />

                        <TextView
                            android:id="@+id/service_tz"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"
                            android:layout_weight="1"
                            android:gravity="right"
                            android:text="@string/service_tz"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_large"
                            android:visibility="invisible" />
                    </LinearLayout>

                    <com.rubu.view.AutoListView
                        android:id="@+id/auto_list_view"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:divider="@android:color/white"
                        android:dividerHeight="0dp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="2dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginTop="@dimen/item_margin_top"
                        android:orientation="horizontal"
                        android:visibility="gone">

                        <TextView
                            android:id="@+id/tv_total_price"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_medium" />
                    </LinearLayout>

                    <include layout="@layout/additional_services" />

                    <TextView
                        android:id="@+id/tv_total_service_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginTop="4dp"
                        android:layout_weight="1.01"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_small" />

                    <include layout="@layout/additional_expenses" />

                    <include layout="@layout/include_service_time" />

                    <include layout="@layout/include_btns_actions" />

                    <include layout="@layout/include_handler_question" />

                    <include layout="@layout/include_service_code" />

                    <include layout="@layout/include_add_imgs" />

                    <TextView
                        android:id="@+id/tv_sign_hint"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/item_margin"
                        android:layout_marginRight="@dimen/item_margin"
                        android:layout_marginTop="@dimen/item_margin_top"
                        android:background="@drawable/bg_pick_finsih"
                        android:gravity="left"
                        android:padding="10dp"
                        android:textColor="@color/red_dark"
                        android:visibility="gone" />

                    <Button
                        android:id="@+id/btn_next"
                        android:layout_width="match_parent"
                        android:layout_height="44dp"
                        android:layout_marginLeft="@dimen/item_margin"
                        android:layout_marginRight="@dimen/item_margin"
                        android:layout_marginTop="4dp"
                        android:background="@color/blue"
                        android:text="接单"
                        android:textColor="@android:color/white"
                        android:textSize="@dimen/text_large" />

                    <com.rubu.view.AutoListView
                        android:id="@+id/list_view_timeline"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/item_margin"
                        android:layout_marginRight="@dimen/item_margin"
                        android:divider="@android:color/white"
                        android:dividerHeight="0dp" />
                </LinearLayout>

            </LinearLayout>
        </ScrollView>
    </com.rubu.view.PullDownScrollView>
</LinearLayout>