<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_36"
    android:layout_marginBottom="@dimen/dp_1"
    android:layout_marginLeft="@dimen/check_order_item_margin_left"
    android:layout_marginRight="@dimen/check_order_item_margin_right"
    android:layout_marginTop="6dp"
    android:background="@drawable/bg_check_title"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/order_Title_Id"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/check_order_item_padding_left"
        android:text="工单id"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_14" />

    <TextView
        android:id="@+id/id_name"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/check_order_item_padding_left"
        android:paddingRight="@dimen/check_order_item_padding_right"
        android:text="客户名称"
        android:textColor="@color/black"
        android:textSize="@dimen/sp_14" />


</LinearLayout>
